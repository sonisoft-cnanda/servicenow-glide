{
  "spinnerStream": "stdout",
  "progressStream": "stdout",
  "reasonerStream": "stderr",
  "options": [
    {
      "mode": "create",
      "project": "./tsconfig.json",
      "startFrom": "./src",
      "exclude": [
        "src/imports/**/*.ts"
      ],
      "include":[
        "src/**"
      ],
      "noBackup":true,
      "exportFilename": "index.ts",
      "useSemicolon": true,
      
      "overwrite": true
    }
  ]
}